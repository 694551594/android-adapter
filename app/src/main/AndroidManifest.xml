<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.yhq.adapter.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpandableListViewSingleActivity"></activity>
        <activity android:name=".RecyclerListViewSingleActivity"></activity>
        <activity android:name=".ListViewMutipleActivity"></activity>
        <activity android:name=".ListViewSingleActivity"></activity>
        <activity android:name=".RecyclerListViewMutipleActivity"></activity>
        <activity android:name=".ExpandableListViewMutipleActivity"></activity>
    </application>

</manifest>